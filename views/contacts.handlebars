<div class="container">
<h1>Contacts</h1>
{{#each contacts}}

  <div class="row">
    <div class="col s12 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"> {{first_name}} </span>
          <p>{{message}}</p>
        </div>
        <div class="card-action">
          <small>{{date}}</small>
        </div>
      </div>
    </div>
  </div>
   {{else}}
   <p>No contacts found</p>
   {{/each}}
</div>